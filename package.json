{
  "name": "expo-share-extension",
  "version": "3.0.0-0",
  "description": "Expo config plugin to create an iOS share extension.",
  "main": "build/index.js",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.js"
    },
    "./metro": {
      "require": "./build/metro.cjs",
      "import": "./build/metro.cjs"
    },
    "./app.plugin": {
      "require": "./app.plugin.js",
      "import": "./app.plugin.js"
    }
  },
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "open -a \"Xcode\" example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "release": "release-it",
    "ncu": "npx npm-check-updates --deep -u"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-share-extension",
    "ExpoShareExtension"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxAst/expo-share-extension.git"
  },
  "bugs": {
    "url": "https://github.com/MaxAst/expo-share-extension/issues"
  },
  "author": "Max Ast (https://github.com/MaxAst)",
  "license": "MIT",
  "homepage": "https://github.com/MaxAst/expo-share-extension#readme",
  "dependencies": {
    "semver": "^7.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@types/semver": "^7.5.8",
    "expo-module-scripts": "^4.0.2",
    "expo-modules-core": "^2.0.6",
    "release-it": "^17.10.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
